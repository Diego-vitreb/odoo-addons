<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	<record id="sd_hr_timesheet_line_search" model="ir.ui.view">
            <field name="name">sd.hr.analytic.timesheet.search</field>
            <field name="model">hr.analytic.timesheet</field>
            <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_search"/>
            <field name="arch" type="xml">
            	<xpath expr="//filter[@string='Timesheet Month']" position="after">
            		<filter string="Timesheet Day" icon="terp-go-month" domain="[]" context="{'group_by':'date:day'}" help="Timesheet by Day"/>
            	</xpath>
            </field>
        </record>
    </data>
</openerp>