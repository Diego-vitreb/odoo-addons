<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	<record id="inherit_stock_card_move_tree" model="ir.ui.view">
            <field name="name">inherit.stock.card.move.tree</field>
            <field name="model">stock.card.move</field>
            <field name="inherit_id" ref="stock_card.stock_card_move_tree"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='average']" position="attributes">
            		<attribute name="invisible">True</attribute>
            	</xpath>
            	<xpath expr="//field[@name='date']" position="after">
            		<field name="stock_card_product_id" string="Producto"/>
            	</xpath>
            </field>
        </record>
    </data>
</openerp>