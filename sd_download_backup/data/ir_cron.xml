<?xml version="1.0" encoding="UTF-8"?>

<openerp>
<data noupdate="1">

    <record id="ir_cron_update_backup_information_scheduler" model="ir.cron">
        <field name="name">Update Backup Information Scheduler</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall"
               eval="(datetime.now () + timedelta (days = 1)).strftime ('%Y-%m-%d 03:00:00')"
               />
        <field name="model">sd.config_dwn_backup</field>
        <field name="function">action_scheduler</field>
    </record>

</data>
</openerp>
