<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data> 
    	<record id="view_sd_dwn_back_tree" model="ir.ui.view">
			<field name="name">sd.dwn.back.tree</field>
			<field name="model">sd.download_backup</field>
			<field name="arch" type="xml">
				<tree string="Download backup">
					<field name="day"/>
					<field name="date"/>
					<field name="size"/>
					<button name="down_back" string="Download" icon="gtk-go-down" type="object"/>
                </tree>
			</field>
		</record>
    	    
    	<record id="sd_dwn_back_action" model="ir.actions.act_window">
            <field name="name">Week Backup</field>
            <field name="view_type">form</field>
            <field name="res_model">sd.download_backup</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="sd_download_backup.view_sd_dwn_back_tree"/>
        </record>
        
        <menuitem name="Backup"
            id="menu_backup"
            groups="sd_download_backup.sd_backup_user_group,base.group_system"
            sequence="510"/>
          
       	<menuitem name="Backup"
            id="submenu_backup"
            parent="sd_download_backup.menu_backup"/>
    	            
		<menuitem action="sd_download_backup.sd_dwn_back_action" name="Download Backup" id="menu_backup_download"
            parent="sd_download_backup.submenu_backup"/>
                  	        
    </data>
</openerp>