<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="report.external_layout_header">
			<t t-name="report.external_layout_header">
				<div class="header">
					<div class="row">
						<div class="col-xs-3">
							<img t-if="company.logo" t-att-src="'data:image/png;base64,%s' % company.logo"
								style="max-height: 120px;" />
						</div>
						<div class="pull-right">
							<div t-field="company.partner_id"
								t-field-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;name&quot;], &quot;no_marker&quot;: true}"
								style="font-size:11px" />
							<div style="font-size:11px">
								NIF:
								<span t-field="company.partner_id.vat" />
							</div>
							<div style="font-size:11px" t-field="company.partner_id.street" />
							<div style="font-size:11px">
								<span t-esc="'%s ' % company.partner_id.zip" />
								<span t-esc="'%s, San Miguel de Abona' % company.partner_id.city" />
							</div>
							<div style="font-size:11px" t-field="company.partner_id.country_id.name" />
							<div style="font-size:11px" t-field="company.partner_id.phone" />
							<div style="font-size:11px" t-field="company.partner_id.email" />
						</div>
						<div class="col-xs-pull-4">
							<br />
							<div style="font-size:30px; font-family:entypoRegular;">SELIMGRAHOTEL</div>
							<div class="col-xs-4 text-center">
								<div style="border-bottom: 1px solid black;" />
								<div style="font-size:9px">Limpieza de sistemas de extracci√≥n</div>
							</div>
						</div>
					</div>
					<div class="row zero_min_height">
						<div class="col-xs-13">
							<br />
							<div style="border-bottom: 1px solid black;" />
						</div>
					</div>
				</div>
			</t>
		</template>
	</data>
</openerp>